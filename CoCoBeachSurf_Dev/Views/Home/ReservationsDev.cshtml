                    $("#Pcklctntpe_" + TripNumber).empty(); 
                    $("#Drplctntpe_" + TripNumber).empty()
                    $("#Pcklctntpe_" + TripNumber).append($("<option>", {
                        value: "0",
                        text: "Select"
                    }));
                    $("Drplctntpe_" + TripNumber).append($("<option>", {
                        value: "0",
                        text: "Select"
                    }));
                    $.each(data, function (i, item) {

                        $("#Pcklctntpe_" + TripNumber).append($("<option>", {
                            value: item.lctn_tpe,
                            text: item.lctn_tpe_dsc
                        }));
                        $("#Drplctntpe_" + TripNumber).append($("<option>", {
                            value: item.lctn_tpe,
                            text: item.lctn_tpe_dsc
                        }));
                    });
                });
            });
    }
