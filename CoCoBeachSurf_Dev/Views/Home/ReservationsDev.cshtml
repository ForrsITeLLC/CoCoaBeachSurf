    function pckgetLocationsByType(data) {
        $.get("/Home/GetLocationsByType",
            {
                type: data
            },
            function (data, status) {
                let d = data;
                $.each(data, function () {
                    $("#pcklctn_" + TripNumber).empty();
                    $("#pcklctn_" + TripNumber).append($("<option>", {
                        value: "0",
                        text: "Select"
                    }));
                    $.each(data, function (i, item) {
                        $("#pcklctn_" + TripNumber).append($("<option>", {
                            value: item.lctn_id,
                            text: item.lctn_nme
                        }));
                    });
                });
            });
    }

