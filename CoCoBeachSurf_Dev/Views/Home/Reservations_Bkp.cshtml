<script>
    var Arrivetime = "";
    var diffDays = "";
    $(document).ready(function () {
        var now = new Date();
        var mm = "";
        if ((now.getMonth() + 1) < 10) {
            mm = '0' + (now.getMonth() + 1);

        }
        else {
            mm = (now.getMonth() + 1);
        }
        var dd = "";
        if ((now.getDate()) < 10) {
            dd = '0' + (now.getDate());

        }
        else {
            dd = (now.getDate());
        }

        var today = now.getFullYear() + '-' + mm + '-' + dd;
        var tomorrow = now.getFullYear() + '-' + mm + '-' + (now.getDate() + 1);
        tomorrow = new Date(`${(new Date()).getFullYear()}-${(new Date()).getMonth() + 1}-${(new Date()).getDate() + 1}`);
        var mm1 = "";
        if ((tomorrow.getMonth() + 1) < 10) {
            mm1 = '0' + (tomorrow.getMonth() + 1);

        }
        else {
            mm1 = (tomorrow.getMonth() + 1);
        }
        var dd1 = "";
        if ((tomorrow.getDate()) < 10) {
            dd1 = '0' + (tomorrow.getDate());

        }
        else {
            dd1 = (tomorrow.getDate());
        }
        tomorrow = tomorrow.getFullYear() + '-' + mm1 + '-' + dd1;
        $('#datePicker').val(today);
        $('#ArrivalSelected').val(today);
        $('#ReturnSelected').val(tomorrow);

        document.getElementById("Arrivingdatetime").innerHTML = new Date().toDateString() + " at " + $('#ddlArriveHr').val() + " " + $('#ddlArriveMinutes').val() + " " + $('#ddlArriveAM').val();
        document.getElementById("Returningdatetime").innerHTML = (new Date(tomorrow)).toDateString();

        var nmbrofDays = "";
        const date1 = new Date(today);
        const date2 = new Date(tomorrow);
        const diffTime = Math.abs(date2 - date1);
        const diffDays = Math.ceil(diffTime / (1000 * 60 * 60 * 24));
        // alert(diffDays);
        DiffDays(today, tomorrow)
        //alert($('#ddlArriveHr').val());
    })
    function DiffDays(date1, date2) {
        const d1 = new Date(date1);
        const d2 = new Date(date2);
        const diffTime = Math.abs(d2 - d1);
        diffDays = Math.ceil(diffTime / (1000 * 60 * 60 * 24));
        return diffDays;
        //alert(diffDays);
    }
    function fnArrivetime() {
        Arrivetime = $('#ddlArriveHr').val() + " " + $('#ddlArriveMinutes').val() + " " + $('#ddlArriveAM').val();
        return Arrivetime;
    }
    $("#Arrivalchange").change(function (event) {
        alert($(this).val());
        Arrivetime = $('#ddlArriveHr').val() + " " + $('#ddlArriveMinutes').val() + " " + $('#ddlArriveAM').val();

    });
    $("#ddlArriveHr").change(function () {
        alert($('#ddlArriveHr').val());
