            },
            function (data, status) {
                let d = data;
                $.each(data, function () {
                    $("#drop_lctn_to").empty();
                    $("#drop_lctn_to").append($("<option>", {
                        value: "0",
                        text: "Select"
                    }));
                    $.each(data, function (i, item) {

